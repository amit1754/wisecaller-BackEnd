{
   "name": "auth-service",
   "version": "1.0.0",
   "description": "",
   "main": "src/index.ts",
   "scripts": {
      "dev": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node src/index.ts --ENVIRONMENT=dev'",
      "build": "rimraf build/ && tsc --build",
      "clean": "rimraf /build && tsc --build --clean",
      "start": "ts-node src/index.ts",
      "pack": "rimraf \"../deploy/auth-service-deploy\" && (xcopy \"build\" \"../deploy/auth-service-deploy\" /S /I) && (xcopy package.json \"../deploy/auth-service-deploy\" /I) && (cd ../deploy/auth-service-deploy && npm install --only=prod)",
      "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@tsconfig/node14": "^1.0.1",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/jsonwebtoken": "^8.5.4",
      "@types/morgan": "^1.9.3",
      "@types/node": "^16.11.8",
      "nodemon": "^2.0.12",
      "prettier": "1.19.1",
      "rimraf": "3.0.0",
      "serverless-plugin-typescript": "^1.1.9",
      "ts-node": "^10.0.0",
      "typescript": "^4.3.5"
   },
   "dependencies": {
      "@wisecaller/authorizer": "^0.0.7",
      "@wisecaller/mongo": "^0.0.2",
      "aws-sdk": "^2.958.0",
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "google-libphonenumber": "^3.2.22",
      "jsonwebtoken": "^8.5.1",
      "mongoose": "^5.13.2",
      "morgan": "^1.10.0",
      "multer": "^1.4.3",
      "multer-s3": "^2.9.0",
      "serverless-http": "^2.7.0",
      "yup": "^0.32.9"
   }
}
