      "@types/morgan": "^1.9.3",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.8",
      "@types/passport": "^1.0.7",
      "@types/passport-jwt": "^3.0.6",
      "@types/passport-local": "^1.0.34",
      "@types/swagger-ui-express": "^4.1.3",
      "nodemon": "^2.0.12",
      "prettier": "^2.4.1",
      "rimraf": "3.0.0",
      "serverless-plugin-typescript": "^1.1.9",
      "ts-node": "^10.4.0",
