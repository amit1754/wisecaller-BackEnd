{
  "name": "subscription-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node src/index.ts --ENVIRONMENT=dev'",
    "deploy": "AWS_PROFILE=wisecaller sls deploy",
    "build": "rimraf build/ && tsc --build",
    "clean": "rimraf /build && tsc --build --clean",
    "start": "ts-node src/index.ts",
    "pack": "rimraf \"../deploy/auth-service-deploy\" && (xcopy \"build\" \"../deploy/auth-service-deploy\" /S /I) && (xcopy package.json \"../deploy/auth-service-deploy\" /I) && (cd ../deploy/auth-service-deploy && npm install)",
    "format": "prettier --write \"src/*/.ts\" \"src/*/.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.0",
    "@types/uuid": "^8.3.4",
    "serverless-plugin-typescript": "^2.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@wisecaller/authorizer": "^0.0.9",
    "@wisecaller/email": "^0.0.4",
    "@wisecaller/mongo": "^0.0.5",
    "@wisecaller/s3": "^0.0.6",
    "@wisecaller/user-service": "^0.0.8",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "handlebars": "^4.7.7",
    "html-pdf": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "multer-s3": "^2.10.0",
    "razorpay": "^2.8.0",
    "rimraf": "^3.0.2",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2"
  }
}
